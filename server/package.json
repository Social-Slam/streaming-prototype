{
  "name": "stream-api",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "git@github.com:SocialSlam/streaming-prototype.git",
  "author": "halvardssm <halvard@simplyundoable.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "socket.io": "^2.3.0"
  },
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "db:start": "docker run -d --rm -p 5000:27017 -v `pwd`/data:/data/db --name socialslam-stream -e MONGO_INITDB_ROOT_USERNAME=socialslam -e MONGO_INITDB_ROOT_PASSWORD=pwd -e MONGO_INITDB_DATABASE=socialslam-stream mongo:latest | true ",
    "db:stop": "docker kill socialslam-stream"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "@types/socket.io": "^2.1.4",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
